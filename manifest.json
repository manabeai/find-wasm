{
  "manifest_version": 3,
  "name": "WASM Detector",
  "version": "1.0",
  "description": "Detects when a website uses WASM.",
  "permissions": ["webRequest", "notifications"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "WebAssembly.png"
  }
}
